{
  "title": "Marvel - Cluster Pulse",
  "services": {
    "query": {
      "list": {
        "0": {
          "query": "_type:node_event",
          "alias": "Node events",
          "color": "#7EB26D",
          "id": 0,
          "pin": true,
          "type": "lucene",
          "enable": true
        },
        "1": {
          "id": 1,
          "color": "#EAB839",
          "alias": "Index events",
          "pin": true,
          "type": "lucene",
          "enable": true,
          "query": "_type:index_event"
        },
        "2": {
          "id": 2,
          "color": "#6ED0E0",
          "alias": "Routing events",
          "pin": true,
          "type": "lucene",
          "enable": true,
          "query": "_type:routing_event"
        }
      },
      "ids": [
        0,
        1,
        2
      ]
    },
    "filter": {
      "list": {
        "0": {
          "type": "time",
          "field": "@timestamp",
          "from": "now-7d",
          "to": "now",
          "mandate": "must",
          "active": true,
          "alias": "",
          "id": 0
        }
      },
      "ids": [
        0
      ]
    }
  },
  "rows": [
    {
      "title": "Cluster Summary",
      "height": "75px",
      "editable": true,
      "panels": [
        {
          "error": false,
          "span": 12,
          "editable": true,
          "type": "marvel.cluster",
          "mode": "nodes",
          "loadingEditor": false,
          "compact": false,
          "title": "Cluster Summary",
          "optin": true
        }
      ],
      "notice": false
    },
    {
      "title": "Timeline",
      "height": "100px",
      "editable": true,
      "collapse": false,
      "collapsable": true,
      "panels": [
        {
          "span": 12,
          "editable": true,
          "type": "histogram",
          "loadingEditor": false,
          "mode": "count",
          "time_field": "@timestamp",
          "auto_int": true,
          "resolution": 100,
          "interval": "30s",
          "fill": 0,
          "linewidth": 3,
          "pointradius": 5,
          "timezone": "browser",
          "spyable": true,
          "zoomlinks": true,
          "bars": true,
          "stack": true,
          "points": false,
          "lines": false,
          "legend": true,
          "show_query": true,
          "legend_counts": true,
          "x-axis": true,
          "y-axis": true,
          "percentage": false,
          "zerofill": true,
          "interactive": true,
          "options": true,
          "derivative": false,
          "scale": 1,
          "tooltip": {
            "value_type": "cumulative",
            "query_as_alias": true
          },
          "grid": {
            "max": null,
            "min": 0
          },
          "title": "Time line"
        }
      ],
      "notice": false
    },
    {
      "title": "Events",
      "height": "150px",
      "editable": true,
      "collapse": false,
      "collapsable": true,
      "panels": [
        {
          "error": false,
          "span": 9,
          "editable": true,
          "type": "table",
          "loadingEditor": false,
          "queries": {
            "mode": "all",
            "ids": [
              0,
              1,
              2
            ]
          },
          "size": 100,
          "pages": 5,
          "offset": 0,
          "sort": [
            "@timestamp",
            "desc"
          ],
          "group": "default",
          "style": {
            "font-size": "9pt"
          },
          "overflow": "min-height",
          "fields": [
            "@timestamp",
            "_type",
            "event",
            "message"
          ],
          "highlight": [],
          "sortable": true,
          "header": true,
          "paging": true,
          "field_list": false,
          "all_fields": false,
          "trimFactor": 300,
          "normTimes": true,
          "spyable": true,
          "localTime": true,
          "timeField": "@timestamp",
          "title": "Cluster events"
        },
        {
          "error": false,
          "span": 3,
          "editable": true,
          "type": "terms",
          "loadingEditor": false,
          "queries": {
            "mode": "all",
            "ids": [
              0,
              1,
              2
            ]
          },
          "field": "_type",
          "exclude": [],
          "missing": false,
          "other": false,
          "size": 10,
          "order": "count",
          "style": {
            "font-size": "10pt"
          },
          "donut": false,
          "tilt": false,
          "labels": true,
          "arrangement": "vertical",
          "chart": "table",
          "counter_pos": "below",
          "spyable": true,
          "title": "Event types"
        }
      ],
      "notice": false
    }
  ],
  "editable": true,
  "failover": false,
  "index": {
    "interval": "day",
    "pattern": "[.marvel-]YYYY.MM.DD",
    "default": "_all"
  },
  "style": "dark",
  "panel_hints": true,
  "pulldowns": [
    {
      "type": "query",
      "collapse": false,
      "notice": false,
      "enable": true,
      "query": "*",
      "pinned": true,
      "history": [
        "event:node_joined OR event:node_left",
        "_type:cluster_event"
      ],
      "remember": 10
    },
    {
      "type": "filtering",
      "collapse": true,
      "notice": false,
      "enable": true
    }
  ],
  "nav": [
    {
      "type": "marvel.registration",
      "editable": true,
      "title": "Registration"
    },
    {
      "type": "timepicker",
      "collapse": false,
      "notice": false,
      "enable": true,
      "status": "Stable",
      "time_options": [
        "5m",
        "15m",
        "1h",
        "6h",
        "12h",
        "24h",
        "2d",
        "7d",
        "30d"
      ],
      "refresh_intervals": [
        "5s",
        "10s",
        "30s",
        "1m",
        "5m",
        "15m",
        "30m",
        "1h",
        "2h",
        "1d"
      ],
      "timefield": "@timestamp",
      "now": true,
      "filter_id": 0
    },
    {
      "type": "marvel.navigation",
      "source": "url",
      "url": "../common/marvelLinks.json",
      "editable": false
    },
    {
      "type": "marvel.save",
      "editable": true,
      "title": "Restore"
    }
  ],
  "loader": {
    "show_home": false,
    "save_gist": false,
    "save_elasticsearch": false,
    "save_local": false,
    "save_default": false,
    "save_temp": false,
    "save_temp_ttl_enable": true,
    "save_temp_ttl": "30d",
    "load_gist": false,
    "load_elasticsearch": false,
    "load_elasticsearch_size": 20,
    "load_local": false,
    "hide": true 
  },
  "refresh": "1m"
}
